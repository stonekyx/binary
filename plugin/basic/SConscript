#!/usr/bin/python

Import('qtEnv baseEnv')

qtEnv = qtEnv.Clone()
baseEnv = baseEnv.Clone()

qtEnv.EnableQt4Modules([
    'QtGui',
    'QtCore'
    ])

qtEnv.Moc4('MainWindow.h')

pluginTarget = qtEnv.SharedLibrary(target='basic', source=Glob('*.cpp')+Glob('*.cc'))
Install(qtEnv['LOCAL_DATADIR'], pluginTarget)
